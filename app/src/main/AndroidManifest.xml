<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="net.frostedbytes.android.trendo">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_trends_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_trends_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme"
    android:fullBackupContent="@xml/backup_descriptor">
    <activity
      android:name="net.frostedbytes.android.trendo.MainActivity"
      android:label="@string/app_name"/>
    <activity android:name="net.frostedbytes.android.trendo.SignInActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name="net.frostedbytes.android.trendo.BaseActivity"/>
  </application>

</manifest>
